<span>
                <SplitButton title={this.state.regionKey || "Region"} id="region-nested-dropdown" onSelect={(e) =>
                  this.setState({
                    region: region[e],
                    regionKey: e
                  })
                }>
                  {Object.keys(region).map((key, index) => {
                    return <MenuItem eventKey={key}>{key} ({region[key]})</MenuItem>
                  })}
                </SplitButton>
              </span>
              <span style={{ marginLeft: 20 }}>
                <SplitButton title={this.state.customType || "customerType"} id="type-nested-dropdown" onSelect={(e) => this.setState({ customType: e })} >
                  {
                    Object.keys(customerType).map(function (key, index) {
                      return <MenuItem eventKey={customerType[key]}>{key} ({customerType[key]})</MenuItem>
                    })
                  }
                </SplitButton>
              </span>
              <span style={{ marginLeft: 20 }}>
                <Button onClick={() => { this.generate() }} bsStyle="primary">OK</Button>
              </span>




          const arr = results.data.filter((a) => {
            return region == a[1] && customType == a[0]
          })